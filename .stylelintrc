{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-no-unsupported-browser-features"
  ],
  "rules": {
    "selector-class-pattern": null,
    "no-descending-specificity": null
  },
  "ignoreFiles": [
    "**/*.ts",
    "**/*.tsx",
    "dist/**",
    "examples/components-list/dist/**",
    "src/theme/presets/**/*.css"
  ]
}